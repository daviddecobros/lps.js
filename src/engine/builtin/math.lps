max(X, Y, Z) <-
  Z = max(X, Y).
min(X, Y, Z) <-
  Z = min(X, Y).

abs(X, V) <-
  is_variable(V),
  V = abs(X).
abs(X, V) <-
  not is_variable(V),
  V == abs(X).

